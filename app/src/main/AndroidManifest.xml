<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.biz.myshop.birthdays">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Birthdays"
            android:label="@string/Birthdays">
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".wiki_person_info"
            android:label="@string/wiki_person_info">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".star_list" />
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".star_list"
            android:label="@string/star_list">
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".Central"
            android:label="@string/title_activity_central"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>